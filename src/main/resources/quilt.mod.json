{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "A Quilt mod that fixes camera clipping.",
      "contributors": {
        "SDFTDusername": "Programmer"
      },

      "contact": {
        "homepage": "https://sdftdusername.github.io/",
        "issues": "https://github.com/SDFTDusername/FixCameraClipping/issues",
        "sources": "https://github.com/SDFTDusername/FixCameraClipping/"
      },

      "icon": "assets/fixcameraclipping/icon.png"
    },

    "entrypoints": {
      "init": "io.github.sdftdusername.fixcameraclipping.FixCameraClipping"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": "*"
      },
      {
        "id": "cosmicreach",
        "versions": "*"
      }
    ]
  },

  "mixin": "fixcameraclipping.mixins.json"
}
